{
  "name" : "Stat View",
  "version" : "2.0.5",
  "description" : "View important app engine admin stats",
  "permissions": [
      "tabs",
      "storage",
      "cookies",
      "https://*/*",
      "http://*/*"
  ],
  "content_security_policy": "script-src 'self' https://googledrive.com https://ssl.google-analytics.com; object-src 'self'",
  "icons": { "16": "graphics/bar-icon.png", "48": "graphics/bar-icon.png", "128": "graphics/bar-icon.png" },
  "browser_action": {
      "default_icon": "graphics/bar-icon.png",
      "default_popup": "view/popup.html"
  },
  "manifest_version": 2
}
