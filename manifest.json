{
  "name" : "Stat View",
  "version" : "1",
  "description" : "View important app engine admin stats",
  "permissions": [
      "tabs",
      "storage",
      "cookies",
      "https://appengine.google.com/*",
      "https://*.appspot.com/*",
      "http://*.xkcd.com/*"
  ],
  "icons": { "16": "graphics/bar-icon.png", "48": "graphics/bar-icon.png", "128": "graphics/bar-icon.png" },
  "browser_action": {
      "default_icon": "graphics/bar-icon.png",
      "default_popup": "view/popup.html"
  },
  "background": {
      "scripts": ["backgroundScripts/DataGrabber.js"],
      "persistent": true
  },
  "content_scripts": [
      {
          "matches": ["http://*.xkcd.com/"],
          "js": ["injectedScripts/Xkcd.js"]
      },
      {
          "matches": ["https://*.appspot.com/_ah/datastore_admin/*"],
          "js": ["injectedScripts/DatastoreAdmin.js"]
      },
      {
          "matches": ["https://appengine.google.com/cron?*"],
          "js": ["injectedScripts/CronJobs.js"]
      },
      {
          "matches": ["https://appengine.google.com/queues?*"],
          "js": ["injectedScripts/TaskQueues.js"]
      },
      {
          "matches": ["https://appengine.google.com/dashboard?*"],
          "js": ["injectedScripts/Dashboard.js"]
      }
  ],
  "manifest_version": 2
}
