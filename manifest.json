{
  "name" : "Stat View",
  "version" : "1",
  "description" : "View important app engine admin stats",
  "permissions": [
      "tabs",
      "storage",
      "cookies",
      "https://appengine.google.com/*",
      "https://*.appspot.com/*",
      "http://*.xkcd.com/*"
  ],
  "icons": { "16": "graphics/bar-icon.png", "48": "graphics/bar-icon.png", "128": "graphics/bar-icon.png" },
  "browser_action": {
      "default_icon": "graphics/bar-icon.png",
      "default_popup": "view/popup.html"
  },
  "manifest_version": 2
}
